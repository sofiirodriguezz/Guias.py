def saldoActual(cuenta:list[tuple]) -> int:
    res:int = 0
    n=0
    while (n<len(cuenta)):
        if n[0]=="I":
            res+= n[1]
        else:
            if n[0]== "R":
                res-=n[1]
        n+=1
    return res 


print(saldoActual([("I",2000),("R",20),("R",1000),("I",300)]))

